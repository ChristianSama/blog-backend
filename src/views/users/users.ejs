<%- include("../partials/top"); %> 
<div class="users">
  <h1>Users</h1>
  <% for (user of users) { %>
    <div class="card mt-3" style="width: 30rem">
      <div class="card-body">
        <h5 class="card-title"><%= user.name %> <%= user.lastname %></h5>
        <h6 class="card-subtitle mb-2 text-muted"><%= user.email %></h6>
      </div>
    </div>
  <% } %>
</div>
<%- include("../partials/bottom"); %>
